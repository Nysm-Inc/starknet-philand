{
  "name": "starknet-philand",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "tsc": "tsc",
    "store-metadata": "run-s tsc store-metadata:run",
    "store-metadata:run": "node ./build/scripts/storage/metadata.js",
    "format": "prettier --check \"./**/*.ts\"",
    "format:fix": "prettier --write \"./**/*.ts\"",
    "lint": "eslint --ext .ts test scripts hardhat.config.ts",
    "lint:fix": "yarn lint --fix",
    "compile:l1": "hardhat compile",
    "compile:l2": "hardhat starknet-compile",
    "compile": "yarn compile:l1 && yarn compile:l2",
    "chain:l1": "hardhat node",
    "chain:l2": "starknet-devnet",
    "setBalance": "hardhat setBalance --network fork",
    "setDaiBalance": "hardhat setDaiBalance --network fork",
    "deploy-fakeloot:goerli": "hardhat run --network goerli ./scripts/deployer/deploy-fakeloot.ts",
    "deploy-subdomain:goerli": "hardhat run --network goerli ./scripts/deployer/deploy-subdomain.ts",
    "deploy-deployer:goerli": "STARKNET_NETWORK=alpha hardhat run --network goerli ./scripts/deployer/deploy-deployer.ts",
    "deploy-bridge:goerli": "STARKNET_NETWORK=alpha hardhat run --network goerli ./scripts/deployer/deploy-bridge.ts",
    "deploy-deployer:fork": "hardhat run --network fork ./scripts/deployer/deploy-deployer.ts",
    "deploy-bridge:fork": "hardhat run --network fork ./scripts/deployer/deploy-bridge.ts",
    "account:create": "hardhat account:create --network goerli",
    "account:get": "hardhat account:get --network goerli",
    "call:l1": "hardhat call:l1 --network goerli",
    "call:l2": "hardhat call:l2 --network goerli",
    "invoke:l2": "hardhat invoke:l2 --network goerli",
    "test:l1": "NODE_ENV=test hardhat test",
    "test:l2": "pytest ./test/l2/* --disable-pytest-warnings",
    "test:fix": "yarn lint:fix && yarn format:fix && yarn test:l1 && yarn typecheck"
  },
  "devDependencies": {
    "@codechecks/client": "^0.1.11",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@defi-wonderland/smock": "^2.0.7",
    "@ethersproject/abi": "^5.4.1",
    "@ethersproject/abstract-signer": "^5.4.1",
    "@ethersproject/bignumber": "^5.4.1",
    "@ethersproject/bytes": "^5.4.0",
    "@ethersproject/providers": "^5.4.5",
    "@influenceth/cairo-math-64x61": "^1.2.0",
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@nomiclabs/hardhat-etherscan": "^2.1.6",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@types/chai": "^4.2.21",
    "@types/elliptic": "^6.4.14",
    "@types/fs-extra": "^9.0.12",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.7.10",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "chai": "^4.3.4",
    "commitizen": "^4.2.4",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "ethereum-waffle": "^3.4.0",
    "ethers": "^5.5.1",
    "fs-extra": "^10.0.0",
    "hardhat": "^2.6.2",
    "hardhat-gas-reporter": "^1.0.4",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "mocha": "^9.2.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "prettier-plugin-solidity": "^1.0.0-beta.17",
    "shelljs": "^0.8.4",
    "solhint": "^3.3.6",
    "solhint-plugin-prettier": "^0.0.5",
    "solidity-coverage": "^0.7.17",
    "ts-generator": "^0.1.1",
    "ts-node": "^10.2.1",
    "typechain": "^5.1.2",
    "typescript": "^4.4.2"
  },
  "dependencies": {
    "@ensdomains/ens-contracts": "^0.0.8",
    "@ensdomains/eth-ens-namehash": "^2.0.15",
    "@eth-optimism/smock": "^1.1.10",
    "@ethersproject/abstract-provider": "^5.4.1",
    "@makerdao/hardhat-utils": "^0.1.3",
    "@nomiclabs/ethereumjs-vm": "^4.2.2",
    "@nomiclabs/hardhat-web3": "^2.0.0",
    "@openzeppelin/contracts": "^4.5.0",
    "@shardlabs/starknet-hardhat-plugin": "^0.4.1",
    "@types/yargs": "^17.0.7",
    "eslint-config-typestrict": "^1.0.2",
    "eslint-plugin-no-only-tests": "^2.6.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sonarjs": "^0.10.0",
    "eslint-plugin-unused-imports": "^1.1.5",
    "ethereumjs-utils": "^5.2.5",
    "lodash": "^4.17.21",
    "nft.storage": "^6.0.0",
    "starknet": "^3.1.0",
    "web3": "^1.7.0",
    "yargs": "^17.3.0"
  }
}
